{
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["import", "react", "prettier"],
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended"
  ]
}
